{% schema %}
  {
    "name": "TW Img Text Video Swiper",
    "class": "tailwind-img-text-video-swiper",
	"presets": [
		{
			"name": "TW Img Text Video Swiper"
		}
	],
    "settings": [
        {
            "type": "html",
            "id": "country_iso",
            "label": "Only display if current iso is (* for all)",
            "info": "Comma seperated",
            "default":"*"
        },
		{
			"type": "text",
			"id": "section_wrapper_classes",
			"label": "Section wrapper_classes"
		},
		{
			"type": "select",
			"id": "is_swiper",
			"label": "Is Swiper",
            "options": [
              {
                "value": "true",
                "label": "True"
              },
              {
                "value": "false",
                "label": "False"
              }
			],
			"default": "true"
		},
		{
			"type": "select",
			"id": "lazy_images",
			"label": "Lazy load images?",
