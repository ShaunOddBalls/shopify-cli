{% comment %} Split desktop highlights into an array {% endcomment %}
{% assign desktop_highlights = section.settings.desktop_highlights | split: "," %}

{% schema %}
{
  "name": "Header",
  "settings": [
    {
      "type": "image_picker",
      "id": "logo",
      "label": "Logo"
    },
    {
      "type": "link_list",
      "id": "menu",
      "label": "Menu",
      "default": "main-menu"
    },
    {
      "type": "textarea",
      "id": "desktop_highlights",
      "label": "Desktop hightlights",
      "info": "Used for adding highlights to the nav options. Format Title|Font Colour|Background Colour. Seperate with comma."
    }
  ],
  "blocks": [
    {
      "type": "nav_image",
      "name": "Navigation Image",
      "settings": [
        {
          "type": "text",
          "id": "menu_item",
          "label": "Menu Item Text",
          "info": "Enter the exact text of the menu item this image should appear under"
        },
        {
          "type": "image_picker",
          "id": "image",
          "label": "Image"
        },
        {
          "type": "url",
          "id": "link",
          "label": "Link"
        },
        {
          "type": "text",
          "id": "heading",
          "label": "Heading"
        }
      ]
    }
  ]
}
{% endschema %}


