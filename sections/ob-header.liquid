      <div class="flex justify-center h-full">
        <a class="flex  align-center" href="{{ routes.root_url }}">
          {% if section.settings.logo %}
            {{ section.settings.logo | image_url: width: 150 | image_tag: class: 'h-12 self-center min-w-16', alt: shop.name }}
          {% else %}
            <span class="h-12 self-center">{{ shop.name }}</span>
          {% endif %}
        </a>
        <div class="grid items-center grid-cols-1 [&_>_*]:row-start-1 [&_>_*]:col-start-1">
          <div class="search-collapse flex items-center justify-center w-full h-full">
            <nav class="flex h-full relative">
              <ul class="flex  p-0 m-0 z-40" role="menubar">
                {% for link in linklists[section.settings.menu].links %}
                  {% assign highlight = 'no' %}
                  {% assign styles = '' %} <!-- Initialize styles outside the loop -->
                  
                  {% for highlight in desktop_highlights %}
                    {% assign highlight_parts = highlight | split: '|' %} <!-- Split the highlight data into parts -->
                    {%- assign link_name = highlight_parts[0] | downcase -%}
                    {%- assign link_title = link.title | downcase -%}
                    {% if link_name == link_title %}
            
                      {% assign highlight = 'yes' %}
                     
                      {% capture styles %}background-color: {{ highlight_parts[2] }}; color: {{ highlight_parts[1] }}{% endcapture %}
                    {% endif %}
                  {% endfor %}
                
                  <li class="list-none{% if link.links.size > 0 %} has-submenu{% endif %}" role="menuitem">
                    <button class="h-full m-0 bg-transparent border-0" aria-expanded="false">
                      <div class="flex relative items-center justify-center h-full py-2 lg:mx-1 xl:mx-1 ">
                        <a class="typography typography--title-six py-2 sm:px-3 xl:px-3 uppercase !text-[10px] 2xl:!text-[12px] whitespace-nowrap" href="{{ link.url }}" {% if highlight == "yes" %} style="{{ styles }}{% endif %}">{{ link.title }}</a>
                      </div>
                    </button>
                    {% if link.links.size > 0 %}
                      <div class="fixed left-0 right-0 hidden" aria-hidden="true" role="menu">
                        <div class="relative">
                          <div class="absolute inset-0 bg-white -z-10"></div>
                          <div class="wrapper wrapper--size-sm z-10 py-12">
                            <div class="flex items-start justify-between gap-20">
                              {% for childlink in link.links %}
                                <div class="flex  gap-20">
                                  <div role="group">
                                    <p class="typography typography--body-md mb-1 uppercase font-semibold">{{ childlink.title }}</p>
                                    {% if childlink.links.size > 0 %}
                                      <ul class="flex flex-col p-0 m-0 list-none">
                                        {% for grandchildlink in childlink.links %}
                                          <li class="py-1">
                                            <a class="typography typography--body-md font-semibold w-full text-[#282828] no-underline" href="{{ grandchildlink.url }}">
                                              <p class="typography typography--body-md">{{ grandchildlink.title }}</p>
                                            </a>
                                          </li>
                                        {% endfor %}
                                      </ul>
                                    {% endif %}
                                  </div>
                                </div>
                              {% endfor %}
                              {% for block in section.blocks %}
                                {% if block.type == 'nav_image' and block.settings.menu_item == link.title %}
                                  <div class="flex  gap-20">
                                    <div role="group">
                                      <p class="typography typography--body-md mb-4 uppercase font-semibold">{{ block.settings.heading }}</p>
                                      <a href="{{ block.settings.link }}">
                                        {{ block.settings.image | image_url: width: 300 | image_tag: width: 300, height: 300, alt: block.settings.heading }}
                                      </a>
                                    </div>
                                  </div>
                                {% endif %}
                              {% endfor %}
                            </div>
                          </div>
                        </div>
                      </div>
                    {% endif %}
                  </li>
                {% endfor %}
                <div class="underline hidden bg-black h-1 absolute bottom-[-1px] left-0 w-0 z-50 duration-300 pointer-events-none"></div>
              </ul>
            </nav>
            <div class="menu-backdrop top-[var(--header-height)]  fixed inset-0 bg-black/80 z-[-1] hidden"></div>
          </div>
          <div class="search-collapse hidden flex items-center justify-center w-full h-full flex-[2_1_auto]">
            <form class="flex items-center w-full gap-4">
              <div class="input-group input-group--has-value w-full [&_.input-group\_\_control]:rounded-full [&_input]:py-1 [&_input]:px-2 [&_input]:text-sm">
                <div class="input-group__control">
                  <div class="input-group__adornment input-group__adornment--placement-start">
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" class="icon" fill="currentColor" viewBox="0 0 24 24" height="16"><path fill-rule="evenodd" clip-rule="evenodd" d="M19.6 10.5C19.6 15.5258 15.5258 19.6 10.5 19.6C5.47421 19.6 1.4 15.5258 1.4 10.5C1.4 5.47421 5.47421 1.4 10.5 1.4C15.5258 1.4 19.6 5.47421 19.6 10.5ZM17.4222 18.3953C15.5741 20.0169 13.1519 21 10.5 21C4.70101 21 0 16.299 0 10.5C0 4.70101 4.70101 0 10.5 0C16.299 0 21 4.70101 21 10.5C21 13.1519 20.0169 15.5741 18.3953 17.4222C18.4304 17.4462 18.4638 17.4739 18.495 17.505L23.495 22.505C23.7683 22.7784 23.7683 23.2216 23.495 23.495C23.2216 23.7683 22.7784 23.7683 22.505 23.495L17.505 18.495C17.4739 18.4638 17.4462 18.4304 17.4222 18.3953Z"></path></svg>
                  </div>
                  <div class="input-group__input">
                    <input class="search-collapse-input" autocomplete="off" placeholder="Search products" aria-label="Search" name="search-products" />
                  </div>
                </div>
              </div>
              <button class="button button--variant-ghost button--icon bg-[#F5F5F5] !w-6 !h-6" data-toggle="collapse" data-target=".search-collapse" data-reset="true" type="reset" aria-label="Clear">
                <svg xmlns="http://www.w3.org/2000/svg" class="icon" fill="none" viewBox="0 0 14 14">
                  <rect width="16.9705" height="1.13137" rx="0.565684" transform="matrix(0.707106 -0.707108 0.707106 0.707108 0.600586 12.6001)" fill="currentColor"></rect>
                  <rect width="16.9705" height="1.13137" rx="0.565684" transform="matrix(-0.707106 -0.707107 0.707106 -0.707108 12.6006 13.4001)" fill="currentColor"></rect>
                </svg>
              </button>
            </form>
          </div>
        </div>
        <div class="flex  items-stretch">
          <div class="search-collapse flex relative items-center justify-end  desktop-navigation__actions">
            <button data-toggle="collapse" data-target=".search-collapse, .search-suggestions" data-overflow="true" aria-expanded="false" aria-haspopup="dialog" aria-label="Open" class="button button--variant-ghost button--size-lg button--icon">
              <svg class="icon" height="24" viewbox="0 0 24 24" width="24" xmlns="http://www.w3.org/2000/svg">
                <path clip-rule="evenodd" d="M19.6 10.5C19.6 15.5258 15.5258 19.6 10.5 19.6C5.47421 19.6 1.4 15.5258 1.4 10.5C1.4 5.47421 5.47421 1.4 10.5 1.4C15.5258 1.4 19.6 5.47421 19.6 10.5ZM17.4222 18.3953C15.5741 20.0169 13.1519 21 10.5 21C4.70101 21 0 16.299 0 10.5C0 4.70101 4.70101 0 10.5 0C16.299 0 21 4.70101 21 10.5C21 13.1519 20.0169 15.5741 18.3953 17.4222C18.4304 17.4462 18.4638 17.4739 18.495 17.505L23.495 22.505C23.7683 22.7784 23.7683 23.2216 23.495 23.495C23.2216 23.7683 22.7784 23.7683 22.505 23.495L17.505 18.495C17.4739 18.4638 17.4462 18.4304 17.4222 18.3953Z" fill-rule="evenodd"></path>
              </svg>
            </button>
            <button aria-expanded="false" aria-haspopup="dialog" aria-label="Account" class="button button--variant-ghost button--size-lg button--icon" data-toggle="collapse" data-target=".account-nav" data-class="clicked" type="button">
              <svg class="icon" height="24" viewbox="0 0 24 24" width="24" xmlns="http://www.w3.org/2000/svg">
                <circle cx="12" cy="6" fill="none" r="5.3" stroke="currentColor" stroke-width="1.4"></circle>
                <path d="M0.7 21.2778C0.7 17.3688 3.86883 14.2 7.77778 14.2H16.2222C20.1312 14.2 23.3 17.3688 23.3 21.2778C23.3 22.3946 22.3946 23.3 21.2778 23.3H2.72222C1.60538 23.3 0.7 22.3946 0.7 21.2778Z" fill="none" stroke="currentColor" stroke-width="1.4"></path>
              </svg>
            </button>
            <div class="absolute overflow-hidden right-0 top-[var(--header-height)] px-6 pb-6"></div>
            <div class="flex items-center justify-center w-12 h-12">
              <button class="button w-6 h-6 p-0 !bg-gray-900" data-toggle="collapse" data-target=".cart-nav" data-class="clicked" data-overflow="true" aria-expanded="false" aria-haspopup="dialog" aria-label="Cart">0</button>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="mobile-navigation fixed top-0 left-0 right-0 z-20 xl:hidden">
    <div class="flex flex sticky top-0 flex-col bg-white h-[var(--mobile-header-height-with-search)] flex-shrink-0 z-10">
      <div class="flex items-center justify-center  px-4">
        <div class="flex ">
          <button class="button button--variant-ghost button--icon" type="button" data-toggle="collapse" data-target=".side-nav" data-class="clicked" data-overflow="true" aria-expanded="false" aria-haspopup="dialog" aria-label="Open">
            <svg class="icon !w-5 !h-5" viewbox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
              <path d="M1 4H23M1 12H23M1 20H23" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5"></path>
            </svg>
          </button>
        </div>
        <a  href="{{ routes.root_url }}">
          {% if section.settings.logo %}
            {{ section.settings.logo | image_url: width: 150 | image_tag: class: 'h-10 self-center', alt: shop.name }}
          {% else %}
            <span class="h-10 self-center">{{ shop.name }}</span>
          {% endif %}
        </a>
        <div class="flex items-center justify-end  h-9">
          <button class="button w-6 h-6 p-0" data-toggle="collapse" data-target=".cart-nav" data-class="clicked" data-overflow="true" aria-expanded="false" aria-haspopup="dialog" aria-label="Cart">2</button>
        </div>
      </div>
      <form class="w-full">
        <div class="input-group input-group--has-value color-[#111111] w-full [&_.input-group\_\_control]:px-0 [&_.input-group\_\_control]:px-2 [&_.input-group\_\_control]:!rounded-[0] [&_.input-group\_\_control]:!border-x-0 [&_.input-group\_\_control]:!border-y-1 [&_.input-group\_\_control]:!border-[#F0F0F0] [&_input]:px-0 [&_input]:py-4">
          <div class="input-group__control">
            <div class="input-group__adornment input-group__adornment--placement-start">
              <svg class="icon" height="16" viewbox="0 0 24 24" width="16" xmlns="http://www.w3.org/2000/svg">
                <path clip-rule="evenodd" d="M19.6 10.5C19.6 15.5258 15.5258 19.6 10.5 19.6C5.47421 19.6 1.4 15.5258 1.4 10.5C1.4 5.47421 5.47421 1.4 10.5 1.4C15.5258 1.4 19.6 5.47421 19.6 10.5ZM17.4222 18.3953C15.5741 20.0169 13.1519 21 10.5 21C4.70101 21 0 16.299 0 10.5C0 4.70101 4.70101 0 10.5 0C16.299 0 21 4.70101 21 10.5C21 13.1519 20.0169 15.5741 18.3953 17.4222C18.4304 17.4462 18.4638 17.4739 18.495 17.505L23.495 22.505C23.7683 22.7784 23.7683 23.2216 23.495 23.495C23.2216 23.7683 22.7784 23.7683 22.505 23.495L17.505 18.495C17.4739 18.4638 17.4462 18.4304 17.4222 18.3953Z" fill-rule="evenodd"></path>
              </svg>
            </div>
            <div class="input-group__input">
              <input readonly data-toggle="collapse" data-target=".search-collapse, .search-suggestions" data-overflow="true" aria-describedby="search-products-helper-text" aria-invalid="false" aria-label="Search products" autocomplete="off" id="search-products" placeholder="Search products" />
            </div>
            <div class="input-group__adornment input-group__adornment--placement-end"></div>
          </div>
        </div>
      </form>
    </div>
  </div>
</header>
 {% endcomment %}
