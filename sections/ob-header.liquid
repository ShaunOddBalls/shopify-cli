        <div class="grid items-center grid-cols-1 [&_>_*]:row-start-1 [&_>_*]:col-start-1">
          <div class="search-collapse flex items-center justify-center w-full h-full">
            <nav class="flex h-full relative">
              <ul class="flex  p-0 m-0 z-40 mx-1" role="menubar">
                {% for link in linklists[section.settings.menu].links %}
                  {%- comment %}<locksmith:7241>{% endcomment -%}
                    {%- capture var %}{% render 'locksmith-variables', scope: 'subject', subject: link, subject_parent: linklists[section.settings.menu], variable: 'transparent' %}{% endcapture %}{% if var == "true" %}{% else %}{% continue %}{% endif -%}
                  {%- comment %}</locksmith:7241>{% endcomment -%}
                  {% comment %} Check for highlights {% endcomment %}
                  {% assign highlight = 'no' %}
                  {% assign styles = '' %}
                  
                  {% for highlight in desktop_highlights %}
                    {% assign highlight_parts = highlight | split: '|' %}
                    {%- assign link_name = highlight_parts[0] | downcase -%}
                    {%- assign link_title = link.title | downcase -%}
                    {% if link_name == link_title %}
                      {% assign highlight = 'yes' %}
                      {% capture styles %}background-color: {{ highlight_parts[2] }}; color: {{ highlight_parts[1] }}{% endcapture %}
                    {% endif %}
                  {% endfor %}
                
                  <li class="list-none{% if link.links.size > 0 %} has-submenu{% endif %}" role="menuitem">
                    <button class="h-full m-0 bg-transparent border-0" aria-expanded="false">
                      <div class="flex relative items-center justify-center h-full py-2 lg:mx-1 xl:mx-1 ">
                        <a class="typography typography--title-six py-2 sm:px-3 xl:px-3 uppercase !text-[10px] 2xl:!text-[12px] whitespace-nowrap" href="{{ link.url }}" {% if highlight == "yes" %} style="{{ styles }}{% endif %}">{% include 'ob-desktop-nav-label-logic',navtitle:link.title, label_arr:label_arr, level: 1 %}</a>
                      </div>
                    </button>
                    {% comment %} Submenu {% endcomment %}
                    {% if link.links.size > 0 %}
                      {% assign grandchildlinks = link.links[0].links %}
                      <div class="fixed {% if grandchildlinks.size  > 0 %} left-0 right-0 {% endif %} hidden" aria-hidden="true" role="menu">
                        <div class="relative">
                          <div class="absolute inset-0 bg-white -z-10"></div>
                          <div class="wrapper wrapper-sm z-10 py-12">
                            <div class="{% if grandchildlinks.size  > 0 %}flex {% endif %} items-start justify-between gap-20">
                              {% for childlink in link.links %}
                                {%- comment %}<locksmith:bb49>{% endcomment -%}
                                  {%- capture var %}{% render 'locksmith-variables', scope: 'subject', subject: childlink, subject_parent: link, variable: 'transparent' %}{% endcapture %}{% if var == "true" %}{% else %}{% continue %}{% endif -%}
                                {%- comment %}</locksmith:bb49>{% endcomment -%}
                                <div class="flex  gap-20">
                                  <div role="group">
                                    {% if childlink.links.size > 0 %}
                                    <p class="typography typography--body-md mb-1 uppercase font-semibold">{% include 'ob-desktop-nav-label-logic',navtitle:childlink.title, label_arr:label_arr, level: 2 %}</p>
                                      <ul class="flex flex-col p-0 m-0 list-none">
                                        {% for grandchildlink in childlink.links %}
                                          {%- comment %}<locksmith:4775>{% endcomment -%}
                                            {%- capture var %}{% render 'locksmith-variables', scope: 'subject', subject: grandchildlink, subject_parent: childlink, variable: 'transparent' %}{% endcapture %}{% if var == "true" %}{% else %}{% continue %}{% endif -%}
                                          {%- comment %}</locksmith:4775>{% endcomment -%}
                                          <li class="py-1">
                                            <a class="typography typography--body-md font-semibold w-full text-[#282828] no-underline 1234" href="{{ routes.grandchildlink.url }}">
                                              <p class="typography typography--body-md whitespace text-nowrap">{% include 'ob-desktop-nav-label-logic',navtitle:grandchildlink.title, label_arr:label_arr, level: 3 %}</p>
                                            </a>
                                          </li>
                                        {% endfor %}
                                      </ul>
                                    {% else %}
                                      <a href="{{ childlink.url }}" >
                                        <p class="typography typography--body-md mb-1 ">{% include 'ob-desktop-nav-label-logic',navtitle:childlink.title, label_arr:label_arr, level: 2 %}</p>
                                      </a>
                                    {% endif %}
                                  </div>
                                </div>
                              {% endfor %}
                              {% comment %} Navigation images {% endcomment %}
