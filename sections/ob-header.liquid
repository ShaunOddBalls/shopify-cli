                                    <div role="group">
                                      <p class="typography typography--body-md mb-4 uppercase font-semibold">{{ block.settings.heading }}</p>
                                      <a href="{{ block.settings.link }}">
                                        {{ block.settings.image | image_url: width: 300 | image_tag: width: 300, height: 300, alt: block.settings.heading }}
                                      </a>
                                    </div>
                                  </div>
                                {% endif %}
                              {% endfor %}
                            </div>
                          </div>
                        </div>
                      </div>
                    {% endif %}
                  </li>
