                          {{ design.design_image | image_url: width: 240, height: 240 }} 240w,
                          {{ design.design_image | image_url: width: 300, height: 300 }} 300w
                          " sizes="(max-width: 720px) 25vw, 8vw"/>
                          </span>
                          <div class="js-prod-json" style="display:none;">{{ prod | json }} </div>
                          </label>
                          </div>
                      {% endcapture %}
                      {% assign all_prints = all_prints | append: the_print %}
                  {% endif %}
              {% endif %}

        
      {% endfor %}
      {% endpaginate %}
      {% endif %}
      {% endif %}
  {% endfor %}{{ all_prints }}