                          <label for="print_{{ design.title | handle }}" data-print="{{ prod.featured_image | img_url: '480x' }}" style="cursor: pointer;cursor: hand;" class="relative  group flex items-center justify-center rounded-md border font-medium uppercase hover:bg-gray-50 focus:outline-none sm:flex-1 cursor-pointer bg-white text-gray-900 shadow-sm" data-active="ring-2 ring-magenta">
                          <input id="print_{{ design.title | handle }}"  type="radio" {% if prod == product %}checked{% endif %} name="product-print-option-{{ styletag | handle }}" value="{{ design.title }}" class="peer sr-only">
