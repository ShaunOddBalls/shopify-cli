      {% assign main_title = item_title[0] %}
      {% assign sub_title = item_title[1] %}
<div class="cart-item flex flex-col relative" data-cart-key="{{ item.key }}">
			  <div class="flex flex-col gap-y-4 px-3 py-6" style="max-width:98%;">
				<div class="grid grid-cols-[auto_1fr] gap-6">
