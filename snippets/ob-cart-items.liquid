  {% assign men_boxer_bundle_block = '' %}
  {% assign men_bamboo_bundle_block = '' %}
  {% assign women_bamboo_bundle_block = '' %}
  {% assign ladies_briefs_bundle_block = '' %}
  {% assign ladies_boxers_bundle_block = '' %}
  {% assign ladies_thongs_bundle_block = '' %}
  {% assign goolies_bundle_block = '' %}
  {% assign socks_bundle_block = '' %}
  {% assign ankle_socks_bundle_block = '' %}
  {% assign bralettes_bundle_block = '' %}
  {% assign seamless_bundle_block = '' %}
  {% assign seamless_full_briefs_bundle_block = '' %}
  {% assign other_items_block = '' %}
  {% assign sas_block = '' %}
  {% assign sas_prods  = "false" %}
