        {% assign has_easy_bundle_id = true %}
        {% break %}
      {% endif %}
    {% endfor %}

    {% comment %}If EasyBundleId exists, get the items property{% endcomment %}
    {% assign easy_bundle_items = '' %}
    {% if has_easy_bundle_id %}
      {% for prop in item.properties %}
        {% if prop.first == 'Items' %}
          {% assign easy_bundle_items = prop.last | split: ',' %}
          {% break %}
        {% endif %}
      {% endfor %}
    {% endif %}

    {% assign has_matching_sub_product = false %}
    {% assign matching_sub_id = '' %}
    {% assign matching_key = '' %}

    {% for prop in item.properties %}
