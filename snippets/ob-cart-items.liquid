{% assign easy_bundle_item_count = 0 %}
{% assign sub_prods = '' %}
{% for item in cart.items %}
  {% assign has_matching_sub_product = false %}
  {% assign matching_sub_id = '' %}
  {% assign matching_key = '' %}

  {% for prop in item.properties %}
    {% if prop.first == '_sub_and_save_subscription' %}
      {% capture sub_entry %}
        {{ prop.last }}~{{ item.variant.title | replace:"~", " " }}~{{ item.key }}~{{ item.product.title }}~{{ item.product | image_url: width: 400 | image_tag: width: nil }}
      {% endcapture %}
      {% assign sub_prods = sub_prods | append: sub_entry | append: ',' %}
    {% endif %}
  {% endfor %}
{% endfor %}

{% assign sub_prods = sub_prods | strip | rstrip: ',' %}

{% assign sub_prods_arr = sub_prods | split: ',' %}

{% capture show_multibuy_upsell %}
 {% render "ob-show-feature-check", metaobject_handle: "cart-multibuy-complete" %}
{% endcapture %}
{% capture show_multibuy_messaging %}
  {% render "ob-show-feature-check", metaobject_handle: "multiby-upsell-messaging" %}
{% endcapture %}
{% assign show_multibuy_messaging = 'true' %}
{% assign show_multibuy_upsell = 'dftruedf' %}

{% assign MENSBRIEFSBUNDLE = 'briefs,classic mens briefs,teen boys briefs' | split: ',' %}

{% assign MENSBOXERSBUNDLE = 'boxer shorts,classic mens boxer shorts,teen boys boxer shorts' | split: ',' %}

{% assign MENSBAMBOOBUNDLE = 'bamboo boxer shorts,' | split: ',' %}

{% assign WOMENSBAMBOOBUNDLE = 'ladies bamboo boxers,' | split: ',' %}

{% assign LADIESBRIEFSBUNDLE = 'low rise briefs,classic ladies briefs,teen girls low rise briefs' | split: ',' %}

{% assign LADIESBOXERSBUNDLE = 'ladies boxers,classic ladies boxers,teen girls boxers' | split: ',' %}

{% assign LADIESTHONGSBUNDLE = 'thong,classic ladies thongs,teen girls thong' | split: ',' %}

{% assign GOOLIESBUNDLE = 'goolies' | split: ',' %}

{% assign SOCKSBUNDLE = 'socks' | split: ',' %}

{% assign ANKLESOCKSBUNDLE = ' ankle socks, ankle socks (recycled)' | split: ',' %}

{% assign BRALETTESBUNDLE = 'bralette,teen girls bralette' | split: ',' %}

{% assign SEAMLESSBUNDLE = 'seamless,teen girls seamless' | split: ',' %}

{% assign SEAMLESSFULLBRIEFSBUNDLE = 'seamless full briefs,' | split: ',' %}

{% assign previously_seen_upsells = '' %}
{% assign block_order = 'MENSBOXERSBUNDLE,MENSBRIEFSBUNDLE,MENSBAMBOOBUNDLE,WOMENSBAMBOOBUNDLE,LADIESBRIEFSBUNDLE,LADIESBOXERSBUNDLE,LADIESTHONGSBUNDLE,GOOLIESBUNDLE'
  | split: ','
%}

{% render 'free-card-banner' %}
{% assign only_has_sub = "false" %}
{% if cart.items.size == 1 %}
