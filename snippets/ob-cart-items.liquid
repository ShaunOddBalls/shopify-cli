                          {% endif %}

                        {% else %}

                          {% render 'cart-item-bundle-split', easy_bundle_items:easy_bundle_items, easy_bundle_item_count: easy_bundle_item_count, title: item.title %}
                        {% endunless %}
					  </div>
					</div>
					{%  if settings.cart_free_card == 'on' and item.properties and item.properties['_Free-card']  %}
                       <div class="flex w-full justify-end">
                        <button class="button button--variant-ghost button--icon border-gray-200 js-remove-from-cart">
