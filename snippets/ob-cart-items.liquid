    {% assign men_boxer_bundle_block = men_boxer_bundle_block | strip %}
    {% assign men_bamboo_bundle_block = men_bamboo_bundle_block | strip %}
    {% assign women_bamboo_bundle_block = women_bamboo_bundle_block | strip %}
    {% assign ladies_briefs_bundle_block = ladies_briefs_bundle_block | strip %}
    {% assign ladies_boxers_bundle_block = ladies_boxers_bundle_block | strip %}
    {% assign ladies_thongs_bundle_block = ladies_thongs_bundle_block | strip %}
