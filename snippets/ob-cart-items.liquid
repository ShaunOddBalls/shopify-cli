					<div class="flex flex-col w-full gap-y-1">
					  <a href="{%- render 'ob-process-international-link', url:item.product.url %}">
						<p class="typography typography--body-md text-base-primary font-semibold">{{ main_title }}</p>
						<p class="typography typography--body-md text-base-primary text-xs">{{ sub_title }}</p>
					  </a>
					  <div class="grid grid-cols-1 grid-flow-dense [&>*]:col-start-1 gap-1">
						<div class="flex gap-x-1">
						  {% if item.properties._cg.length > 0 %}<p class="typography typography--body-sm text-base-primary font-semibold">{{ 0 | money }}</p>
						  {% else %}
                            <!-- <p class="typography typography--body-sm text-base-primary font-semibold">{{ item.line_price | money }}</p> -->
                                  {% render 'ob-line-item-price', item:item %}                                                                                                   
                                 
                            {% endif %}

						  <!-- <div class="{% unless item.original_line_price != item.line_price %}hidden {% endunless %}typography typography--body-sm text-base-primary font-semibold !text-[#C8C8C8] line-through js-cart-item-comp-price">{{ item.original_line_price | money }}</div> -->
						</div>
